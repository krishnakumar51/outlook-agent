# Mobile Outlook Agent Environment Variables
# Copy this file to .env and fill in your API keys

# LLM Provider API Keys (set the ones you want to use)
GROQ_API_KEY=your_groq_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Appium Configuration (optional, defaults are usually fine)
APPIUM_SERVER_URL=http://localhost:4723

# Outlook App Configuration (optional)
OUTLOOK_APP_PACKAGE=com.microsoft.office.outlook
OUTLOOK_APP_ACTIVITY=.MainActivity

# Agent Configuration (optional)
DEFAULT_LLM_PROVIDER=groq
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=2048

# Database Configuration (optional)
DATABASE_PATH=outlook_automation.db

# Logging (optional)
LOG_LEVEL=INFO
